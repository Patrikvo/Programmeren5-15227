@{
    Layout = "~/Views/Shared/MasterLayout.cshtml";
}
<nav class="control-panel">
    <a href="/Home/Index" class="tile">
        <span class="icon-menu2"></span>
        <span class="screen-reader-text">Home</span>
    </a>
    <h1 class="banner">VOS Logboek</h1>
</nav>
<form class="show-room entity" action="/Log/InsertOne" method="post">
    <div class="detail">
        <div class="command-panel">
            <h2 class="banner">Logboek</h2>
            <button type="submit" value="/Log/InsertOne" class="tile">
                <span class="icon-plus"></span>
                <span class="screen-reader-text">Insert One</span>
            </button>
            <a href="/Log/Cancel" class="tile">
                <span class="icon-close"></span>
                <span class="screen-reader-text">Annuleren</span>
            </a>
        </div>
        <fieldset>
            <div>
                <label for="Log-UserName">Gebruikersnaam</label>
                <input id="Log-UserName" name="Log-UserName" type="text" value="" required />
            </div>
            <div>
                <label for="Log-Email">E-mail</label>
                <input id="Log-Email" name="Log-Email" type="email" value="" required/>
            </div>
            <div>
                <label for="Log-Role">Gebruikersrol</label>
                <input id="Log-Role" name="Log-Role" type="text" value="" required/>
            </div>
            <div>
                <label for="Log-ProcedureCode">Procedure code</label>
                <input id="Log-ProcedureCode" name="Log-ProcedureCode" type="text" value="" required/>
            </div>
            <div>
                <label for="Log-ProcedureTitle">Procedure titel</label>
                <input id="Log-ProcedureTitle" name="Log-ProcedureTitle" type="text" value="" required/>
            </div>
            <div>
                <label for="Log-StepTitle">Stap</label>
                <input id="Log-StepTitle" name="Log-StepTitle" type="text" value="" required/>
            </div>
            <div>
                <label for="Log-ActionCode">Actie</label>
                <input id="Log-ActionCode" name="Log-ActionCode" type="text" value="" required/>
            </div>
            <div>
                <label for="Log-CallNumber">Telefoonnummer gebeld</label>
                <input id="Log-CallNumber" name="Log-CallNumber" type="text" value="" required/>
            </div>
            <div>
                <label for="Log-SendNumber">Telefoonnummer gesms'd</label>
                <input id="Log-SendNumber" name="Log-SendNumber" type="text" value="" required/>
            </div>
        </fieldset>
        <div class="feedback"></div>
    </div>
    <aside class="list">
        <table></table>
    </aside>
</form>